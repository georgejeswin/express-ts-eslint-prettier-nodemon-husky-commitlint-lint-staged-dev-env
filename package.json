{
  "name": "express-ts",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon",
    "build": "rimraf ./dist && tsc",
    "lint": "eslint src/**/*.ts",
    "format": "prettier -w .",
    "prettier:check": "prettier --config .prettierrc.json src/**/*.ts --check --ignore-unknown",
    "prettier:write": "prettier --config .prettierrc.json src/**/*.ts --write --ignore-unknown",
    "prepare": "husky install"
  },
  "lint-staged":{
    "*": [
      "commitlint -E HUSKY_GIT_PARAMS"
   ],
    "./src/**/*.+(ts|js)": [
      "npm run lint",
      "npm run prettier:check"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgejeswin/express-ts-seed-eslint-prettier-husky-commitlint.git"
  },
  "keywords": [
    "Nodejs",
    "typescript",
    "express"
  ],
  "author": "Jeswin George",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
